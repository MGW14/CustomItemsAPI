package work.mgnet.customitemsapi.data;

public class CustomRecipe {
	
	public CustomItemStack topleft;
	public CustomItemStack topmid;
	public CustomItemStack topright;
	public CustomItemStack middleleft;
	public CustomItemStack middlemid;
	public CustomItemStack middleright;
	public CustomItemStack downleft;
	public CustomItemStack downmid;
	public CustomItemStack downright;
	
	public CustomItemStack getTopleft() {
		return topleft;
	}
	public CustomItemStack getTopmid() {
		return topmid;
	}
	public CustomItemStack getTopright() {
		return topright;
	}
	public CustomItemStack getMiddleleft() {
		return middleleft;
	}
	public CustomItemStack getMiddlemid() {
		return middlemid;
	}
	public CustomItemStack getMiddleright() {
		return middleright;
	}
	public CustomItemStack getDownleft() {
		return downleft;
	}
	public CustomItemStack getDownmid() {
		return downmid;
	}
	public CustomItemStack getDownright() {
		return downright;
	}
	
}
